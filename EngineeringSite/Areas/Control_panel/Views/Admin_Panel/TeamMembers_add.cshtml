        <!-- General Form Elements -->
        <form>
            <div class="row mb-3">
                <label for="projectName" class="col-sm-2 col-form-label">الاسم</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="projectName" name="projectName" required>
                </div>
            </div>
            <div class="row mb-3">
                <label for="projectName" class="col-sm-2 col-form-label">الوظيفة</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="projectName" name="projectName" required>
                </div>
            </div>
            <div class="row mb-3">
                <label for="location" class="col-sm-2 col-form-label">لينك لينكد ان</label>
